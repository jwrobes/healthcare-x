<div>
    <h2>Your Recommendations</h2>
</div>
<div class="row">
  <!--   <div class="col-sm-8" style="border: 1px solid black"> -->
    <% @health_plans.each do |health_plan| %>
        <div class="panel panel-default plan-name">
        <div class="panel-heading">
        <h4>
            <%= health_plan.plan_marketing_name %> (<%= health_plan.plan_type %>)
         </h4>
        </div>
        <div class="panel-body">
        <div class="row">
            <div class="col-md-6 col-sm-6">
             <h3>Annual Savings: <span class="active">$475</span></h3>
            </div>
            <div class="col-md-6 col-sm-6"><h3>
                Premium for Age 27: <span class="active">$<%= health_plan.premium_adult_individual_age_27 %></span></h3>
            </div>
        </div>
        </div>
       
        </div>
    <% end %>
